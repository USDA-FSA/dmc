{{#> ../layouts/base title='DMC - New Contract' nav2_active="fsa-nav-global__link--active" }}

  {{#*inline "main-block"}}

    <div class="fsa-section">
      <div class="fsa-section__bd">

        <nav aria-label="Breadcrumbs">
					<div class="fsa-stepped-tabs fsa-stepped-tabs--justified">
						<div class="fsa-stepped-tabs__bd">
							<ol class="fsa-stepped-tabs__list">
								<li class="fsa-stepped-tabs__item">
									<a class="fsa-stepped-tabs__label fsa-stepped-tabs__label--active" aria-current="true">
										<span class="fsa-stepped-tabs__text">Search</span>
									</a>
								</li>
								<li class="fsa-stepped-tabs__item">
									<span class="fsa-stepped-tabs__label fsa-stepped-tabs__label--incomplete">
										<span class="fsa-stepped-tabs__text">General Information</span>
									</span>
								</li>
								<li class="fsa-stepped-tabs__item">
									<span class="fsa-stepped-tabs__label fsa-stepped-tabs__label--incomplete">
										<span class="fsa-stepped-tabs__text">Coverage</span>
									</span>
								</li>
								<li class="fsa-stepped-tabs__item">
									<span class="fsa-stepped-tabs__label fsa-stepped-tabs__label--incomplete">
										<span class="fsa-stepped-tabs__text">Producers</span>
									</span>
								</li>
								<li class="fsa-stepped-tabs__item">
									<span class="fsa-stepped-tabs__label fsa-stepped-tabs__label--incomplete">
										<span class="fsa-stepped-tabs__text">Remittance</span>
									</span>
								</li>
								
							</ol>
						</div>
					</div>
				</nav>

				<h1>Search for Dairy Operation</h1>
			
					<div class="fsa-m-t--m">
						<div class="fsa-grid">
							
							<div class="fsa-grid__1/1 fsa-grid__1/2@m">

								<div class="fsa-field">
									<label class="fsa-field__label" for="UNIQUE-ID-select-program-year">Program Year <span class="fsa-field__label-desc">Required</span></label>
									<select class="fsa-select fsa-field__item " id="UNIQUE-ID-select-program-year" aria-describedby="UNIQUE-ID-select-program-year-help" aria-required="true" name="UNIQUE-ID-select-program-year">
										<option value="">Select program year</option>
										<option value="2019">2019</option>
										<option value="2018">2018</option>
										<option value="2017">2017</option>
										<option value="2016">2016</option>
										<option value="2015">2015</option>
										<option value="2014">2014</option>
									</select>
									<!--<span class="fsa-field__help" id="UNIQUE-ID-select-program-year-help">Instructional message here</span>-->
								</div>
								{{> ../components/select/select-state SELECT_ID="UNIQUE-ID-select-state" HELP_ID="UNIQUE-ID-select-state-help" }}
								{{> ../components/select/select-county SELECT_ID="UNIQUE-ID-select-county" HELP_ID="UNIQUE-ID-select-county-help" STATE_CA=true }}
								<div class="fsa-m-t--m">
									<ul class="fsa-form-list" aria-label="toggle show all states">
										<li>
											<span>
												<input class="fsa-checkbox" id="UNIQUE-ID-show-all-states-counties" type="checkbox" name="pies" value="show-all-states-counties" checked="">
												<label for="UNIQUE-ID-show-all-states-counties">Show all States and Counties</label>
											</span>
										</li>
										<li>
											<span>
												<input class="fsa-checkbox" id="UNIQUE-ID-remember-states-counties" type="checkbox" name="pies" value="remember-states-counties">
												<label for="UNIQUE-ID-remember-states-counties">Remember State and County Selected</label>
											</span>
										</li>
									</ul>
								</div>
							</div>
							
							<div class="fsa-grid__1/1 fsa-grid__1/2@m">

								<span class="fsa-text-size--5">Current Selection:</span>
								<div class="fsa-m-t--m">
									<span class="fsa-text-size--4">Crop Year: <strong>2019</strong></span>
								</div>
								<div class="fsa-m-t--m">
									<span class="fsa-text-size--4">State: <strong>South Dakota (46)</strong></span>
								</div>
								<div class="fsa-m-t--m">
									<span class="fsa-text-size--4">County: <strong>Codington (029)</strong></span>
								</div>
								
							</div>
						</div>


					</div>
			</div>
    </div>

		<div class="fsa-section">
      <div class="fsa-section__bd">        
				
				<div class="fsa-stepped-control">
					<div class="fsa-stepped-control__bd">

						<div class="fsa-stepped-control__message" role="status">
							<strong>1</strong> of <strong>8</strong> Steps
						</div>

						<div class="fsa-stepped-control__list">
							<div class="fsa-stepped-control__shim"></div>
					
							<div class="fsa-stepped-control__item">
								<button class="fsa-btn fsa-btn--secondary fsa-btn--large"
									data-behavior="growl-show whiteout-show"
									aria-controls="growl__cancel-new-contract"
									aria-expanded="false"
									type="button">
									Cancel
								</button>
								<!--
								<a role="button" href="index.html" class="fsa-btn fsa-btn--alt fsa-btn--large fsa-stepped-control__btn" type="button">
									Cancel
								</a>
								-->
							</div>
							<div class="fsa-stepped-control__item">
								<a id="demoNextButton" role="button" href="new-contract-search-results.html" class="fsa-btn fsa-btn--primary fsa-btn--large fsa-stepped-control__btn" type="button">
									<span class="fsa-stepped-control__btn-label">Search</span>
								</a>
							</div>

						</div>
					</div>
				</div>



      </div>
    </div>
		

  {{/inline}}

  {{#*inline "hidden-block"}}
    <!-- HIDDEN - FOR MODALS/GROWL CONTENT -->
    {{> ../includes/hidden }}
    <!-- /HIDDEN -->
  {{/inline}}

{{/../layouts/base }}
