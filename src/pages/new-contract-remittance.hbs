    <!-- HIDDEN - FOR MODALS/GROWL CONTENT -->
		{{> ../components/modal/modal-collection-history }}
  	{{> ../components/growl/growl-cancel-new-contract }}
		{{> ../components/growl/growl-new-contract-created }}
